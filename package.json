{
  "name": "matiq-brackets-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run demo",
    "demo": "npm run dev --workspace=@matiq-brackets/demo",
    "storybook": "npm run storybook --workspace=@matiq-brackets/storybook",
    "build": "vite build",
    "build:lib": "echo 'Component library uses TypeScript source files directly - no build needed'",
    "build:demo": "npm run build --workspace=@matiq-brackets/demo", 
    "build:storybook": "npm run build-storybook --workspace=@matiq-brackets/storybook",
    "build:all": "npm run build:lib && npm run build:demo && npm run build:storybook",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "matiq-brackets": "file:./packages/matiq-brackets",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.0",
    "vite": "^5.4.19"
  }
}
